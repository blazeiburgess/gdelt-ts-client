  "dependencies": {
    "@mozilla/readability": "^0.6.0",
    "axios": "^1.10.0",
    "jsdom": "^26.1.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.31.0",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
