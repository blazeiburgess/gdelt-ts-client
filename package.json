    "@typescript-eslint/parser": "^8.37.0",
    "eslint": "^9.31.0",
    "globals": "^13.24.0",
    "jest": "^30.0.4",
    "rimraf": "^5.0.10",
    "ts-jest": "^29.4.0",
    "typescript": "^5.5.3"
  },
  "keywords": [
