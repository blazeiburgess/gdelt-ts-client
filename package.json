{
  "name": "gdelt-ts-client",
  "version": "1.1.0",
  "description": "TypeScript client for the GDELT API with full type support",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "clean": "rimraf dist",
    "prebuild": "npm run clean",
